{
    "imports": [
        "github.com/project-flogo/flow",
        "github.com/project-flogo/flow/activity/subflow",
        "github.com/project-flogo/contrib/activity/actreturn",
        "github.com/project-flogo/contrib/function/string",
        "github.com/SteveNY-Tibco/labs-air-contrib/activity/log",
        "github.com/SteveNY-Tibco/labs-lightcrane-contrib/activity/jsondeserializer",
        "github.com/SteveNY-Tibco/labs-lightcrane-contrib/activity/filereader",
        "github.com/SteveNY-Tibco/labs-air-contrib/activity/dataembedder",
        "github.com/SteveNY-Tibco/labs-air-contrib/function/air"
    ],
    "name": "X_Transform_AddFileContent",
    "description": "",
    "version": "1.0.0",
    "type": "flogo:app",
    "appModel": "1.1.1",
    "feVersion": "2.9.0",
    "triggers": [],
    "resources": [
        {
            "id": "flow:Enrichment",
            "data": {
                "name": "Enrichment",
                "description": "4|8",
                "links": [
                    {
                        "id": 1,
                        "from": "Log",
                        "to": "ParseValue",
                        "type": "expression",
                        "label": "LogtoParseValue",
                        "value": "$property[\"Enrichment.ParseValue\"]"
                    },
                    {
                        "id": 2,
                        "from": "ParseValue",
                        "to": "Reader01",
                        "type": "default"
                    },
                    {
                        "id": 3,
                        "from": "Reader01",
                        "to": "DataEmbedder01",
                        "type": "default"
                    },
                    {
                        "id": 4,
                        "from": "DataEmbedder01",
                        "to": "Next_Flow4",
                        "type": "default"
                    },
                    {
                        "id": 5,
                        "from": "Next_Flow4",
                        "to": "Return01",
                        "type": "default"
                    },
                    {
                        "id": 6,
                        "from": "Log",
                        "to": "Reader02",
                        "type": "label",
                        "label": "Log to CopyOfFileReader"
                    },
                    {
                        "id": 7,
                        "from": "Reader02",
                        "to": "DataEmbedder02",
                        "type": "default"
                    },
                    {
                        "id": 8,
                        "from": "DataEmbedder02",
                        "to": "Next_Flow8",
                        "type": "default"
                    },
                    {
                        "id": 9,
                        "from": "Next_Flow8",
                        "to": "Return02",
                        "type": "default"
                    }
                ],
                "tasks": [
                    {
                        "id": "Log",
                        "name": "Log",
                        "description": "Simple Log Message Activity",
                        "activity": {
                            "ref": "#log",
                            "input": {
                                "Log Level": "INFO",
                                "flowInfo": false,
                                "message": "=string.concat(\"########## Enrichment:File ########## gateway : \", $flow.gateway, \", reading : \", coerce.toString($flow.reading), \", enriched : \", coerce.toString($flow.enriched))"
                            }
                        }
                    },
                    {
                        "id": "ParseValue",
                        "name": "ParseValue",
                        "description": "JSON Deserializer Activity",
                        "activity": {
                            "ref": "#jsondeserializer",
                            "settings": {
                                "schemaFromfile": false,
                                "sample": "",
                                "dataSample": {
                                    "metadata": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"frame_path\":{\"type\":\"string\"}}}",
                                    "value": "",
                                    "fe_metadata": "{\n    \"frame_path\":\"\"\n}"
                                },
                                "defaultValue": ""
                            },
                            "input": {
                                "JSONString": ""
                            },
                            "schemas": {
                                "output": {
                                    "Data": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"frame_path\":{\"type\":\"string\"}}}",
                                        "fe_metadata": "{\"frame_path\":\"\"}"
                                    }
                                },
                                "settings": {
                                    "dataSample": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"frame_path\":{\"type\":\"string\"}}}",
                                        "fe_metadata": "{\n    \"frame_path\":\"\"\n}"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "Reader01",
                        "name": "Reader01",
                        "description": "This activity write incoming object to file system",
                        "activity": {
                            "ref": "#filereader",
                            "settings": {
                                "BaseFolder": "/home/f1/tmp/"
                            },
                            "input": {
                                "FilePattern": "=air.getsubobject($activity[ParseValue].Data, $property[\"Enrichment.FilenameField\"])"
                            },
                            "schemas": {
                                "output": {
                                    "Results": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"Filename\":{\"type\":\"string\"},\"Content\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "[{\"Filename\":\"2\",\"Content\":\"2\"}]"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "DataEmbedder01",
                        "name": "DataEmbedder01",
                        "description": "This activity embed datas to array",
                        "activity": {
                            "ref": "#dataembedder",
                            "settings": {
                                "Targets": "[{\"Name\":\"Data\",\"Type\":\"String\"}]"
                            },
                            "input": {
                                "Consumer": "",
                                "Producer": "Enrichment.File",
                                "TargetData": {
                                    "mapping": {
                                        "Data": "=$activity[Reader01].Results[0].Content"
                                    }
                                },
                                "InputDataCollection": "=$flow.enriched"
                            },
                            "schemas": {
                                "input": {
                                    "TargetData": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Data\":{\"type\":\"string\"}}}",
                                        "fe_metadata": "{\"Data\":\"2\"}"
                                    }
                                },
                                "settings": {
                                    "Targets": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"Name\":{\"type\":\"string\"},\"Type\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "[{\"Name\":\"Data\",\"Type\":\"String\"}]"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "Next_Flow4",
                        "name": "Next_Flow4",
                        "description": "Simple SubFlow Activity",
                        "activity": {
                            "ref": "#subflow",
                            "settings": {
                                "flowURI": "res://flow:Dummy"
                            }
                        }
                    },
                    {
                        "id": "Return01",
                        "name": "Return01",
                        "description": "Simple Return Activity",
                        "activity": {
                            "ref": "#actreturn"
                        }
                    },
                    {
                        "id": "Reader02",
                        "name": "Reader02",
                        "description": "This activity write incoming object to file system",
                        "activity": {
                            "ref": "#filereader",
                            "settings": {
                                "BaseFolder": "/home/f1/tmp/"
                            },
                            "input": {
                                "FilePattern": "=air.airdataselector($flow.gateway, $flow.reading, $flow.enriched, $property[\"Enrichment.FilenameField\"])"
                            },
                            "schemas": {
                                "output": {
                                    "Results": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"Filename\":{\"type\":\"string\"},\"Content\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "[{\"Filename\":\"2\",\"Content\":\"2\"}]"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "DataEmbedder02",
                        "name": "DataEmbedder02",
                        "description": "This activity embed datas to array",
                        "activity": {
                            "ref": "#dataembedder",
                            "settings": {
                                "Targets": "[{\"Name\":\"Data\",\"Type\":\"String\"}]"
                            },
                            "input": {
                                "Consumer": "",
                                "Producer": "Enrichment.File",
                                "TargetData": {
                                    "mapping": {
                                        "Data": "=$activity[Reader02].Results[0].Content"
                                    }
                                },
                                "InputDataCollection": "=$flow.enriched"
                            },
                            "schemas": {
                                "input": {
                                    "TargetData": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Data\":{\"type\":\"string\"}}}",
                                        "fe_metadata": "{\"Data\":\"2\"}"
                                    }
                                },
                                "settings": {
                                    "Targets": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"Name\":{\"type\":\"string\"},\"Type\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "[{\"Name\":\"Data\",\"Type\":\"String\"}]"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "Next_Flow8",
                        "name": "Next_Flow8",
                        "description": "Simple SubFlow Activity",
                        "activity": {
                            "ref": "#subflow",
                            "settings": {
                                "flowURI": "res://flow:Dummy"
                            }
                        }
                    },
                    {
                        "id": "Return02",
                        "name": "Return02",
                        "description": "Simple Return Activity",
                        "activity": {
                            "ref": "#actreturn"
                        }
                    }
                ],
                "metadata": {
                    "input": [
                        {
                            "name": "gateway",
                            "type": "string"
                        },
                        {
                            "name": "reading",
                            "type": "object",
                            "schema": {
                                "type": "json",
                                "value": "{\"id\":{\"type\":\"string\"},\"origin\":{\"type\":\"number\"},\"device\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"valueType\":{\"type\":\"string\"},\"mediaType\":{\"type\":\"string\"}}"
                            }
                        },
                        {
                            "name": "enriched",
                            "type": "array",
                            "schema": {
                                "type": "json",
                                "value": "{\"type\":\"object\",\"properties\":{\"producer\":{\"type\":\"string\"},\"consumer\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"type\":{\"type\":\"string\"}}}"
                            }
                        }
                    ],
                    "output": [],
                    "fe_metadata": {
                        "input": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"gateway\":{\"type\":\"string\"},\"reading\":{\"type\":\"object\",\"properties\":{\"id\":{\"type\":\"string\"},\"origin\":{\"type\":\"number\"},\"device\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"valueType\":{\"type\":\"string\"},\"mediaType\":{\"type\":\"string\"}}},\"enriched\":{\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"producer\":{\"type\":\"string\"},\"consumer\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"type\":{\"type\":\"string\"}}}}}}"
                    }
                }
            }
        }
    ],
    "properties": [
        {
            "name": "Enrichment.ParseValue",
            "type": "boolean",
            "value": true
        },
        {
            "name": "Enrichment.FilenameField",
            "type": "string",
            "value": "FilenameField not set"
        }
    ],
    "connections": {},
    "contrib": []
}